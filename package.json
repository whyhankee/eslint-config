{
	"name": "wescfg",
	"version": "0.0.1",
	"description": "Whyhankee's ESlint (and prettier) Config",
	"main": "src/index.mjs",
	"type": "module",
	"scripts": {
		"test": "pnpm run '/test:.*/'",
		"test:code": "node ./tests/index.test.mjs",
		"test:lint": "eslint . --fix",
		"format": "prettier --write \"**/*.{js,mjs,json,md}\"",
		"prepare": "husky"
	},
	"engines": {
		"node": ">=20.12.0",
		"pnpm": ">=9.4"
	},
	"packageManager": "pnpm@9.4.0",
	"homepage": "https://github.com/whyhankee/eslint-config",
	"bugs": {
		"url": "https://github.com/whyhankee/eslint-config/issues"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/whyhankee/eslint-config.git"
	},
	"files": [
		"README.md",
		"src/index.mjs"
	],
	"dependencies": {
		"@eslint/js": "^9.6.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-simple-import-sort": "^12.1.0",
		"prettier": "^3.3.2",
		"typescript-eslint": "^7.14.1"
	},
	"peerDependencies": {
		"eslint": "^8.57.0",
		"prettier": "^3.3.2"
	},
	"keywords": [
		"eslint",
		"prettier",
		"config",
		"whyhankee"
	],
	"author": "Paul Wijhenke <paul@paulsplace.nl>",
	"license": "ISC",
	"devDependencies": {
		"husky": "9.0.11"
	}
}
