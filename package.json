{
	"name": "wescfg",
	"version": "0.0.6",
	"description": "Whyhankee's ESlint/Prettier config",
	"main": "src/index.mjs",
	"type": "module",
	"scripts": {
		"test": "pnpm run '/test:.*/'",
		"test:code": "node ./tests/index.test.mjs",
		"test:lint": "eslint . --fix",
		"test:format": "prettier --write .",
		"prepare": "husky"
	},
	"bin": {
		"wescfg-init": "./src/init.mjs"
	},
	"engines": {
		"node": ">=16.20.2",
		"pnpm": ">=9.4"
	},
	"packageManager": "pnpm@9.4.0",
	"homepage": "https://github.com/whyhankee/wescfg",
	"bugs": {
		"url": "https://github.com/whyhankee/wescfg/issues"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/whyhankee/wescfg.git"
	},
	"files": [
		"README.md",
		"src/index.mjs"
	],
	"peerDependencies": {
		"eslint": "^8.57.0",
		"prettier": "^3.3.2"
	},
	"dependencies": {
		"@eslint/js": "^9.6.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-simple-import-sort": "^12.1.0",
		"globals": "^15.8.0",
		"prettier": "^3.3.2",
		"typescript-eslint": "^7.14.1"
	},
	"devDependencies": {
		"husky": "^9.0.11"
	},
	"keywords": [
		"eslint",
		"prettier",
		"config",
		"whyhankee"
	],
	"author": "Paul Wijhenke <paul@paulsplace.nl>",
